{
  "name": "dashboard",
  "private": true,
  "version": "1.0.4",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && electron electron/main.js\"",
    "electron:clean": "rm -rf release dist",
    "electron:build": "npm run electron:clean && vite build && electron-builder",
    "electron:preview": "electron electron/main.js"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.908.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tabler/icons-react": "^3.31.0",
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/react-table": "^8.21.2",
    "amazon-cognito-identity-js": "^6.3.15",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.6.2",
    "form-data": "^4.0.4",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.483.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.4.0",
    "recharts": "^2.15.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.14",
    "tw-animate-css": "^1.2.4",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^38.2.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0",
    "wait-on": "^8.0.3"
  },
  "overrides": {
    "react-is": "^19.0.0-rc-69d4b800-20241021"
  },
 "build": {
  "appId": "com.gethealthe.ams",
  "productName": "GHE AMS",
  "files": [
    "dist/**/*",
    "electron/**/*",
    "node_modules/**/*"
  ],
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "mac": {
    "category": "public.app-category.utilities",
    "target": [
      "dmg",
      "zip"
    ],
    "icon": "resources/icon.icns",
    "identity": null,
    "hardenedRuntime": false,
    "gatekeeperAssess": false
  },
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      },
      {
        "target": "portable",
        "arch": [
          "x64"
        ]
      }
    ],
    "icon": "resources/icon.ico"
  },
  "linux": {
    "target": [
      "AppImage",
      "deb"
    ],
    "category": "Utility",
    "icon": "resources/icon.png"
  },
  "publish": {
    "provider": "github",
    "owner": "ethan-ghe",
    "repo": "AMS",
    "releaseType": "release"
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true
  }
}
}